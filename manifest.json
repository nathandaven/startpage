{
  "manifest_version": 2,
  "name": "Nathan Start Page",
  "short_name": "Nathan Start Page",
  "version": "7.0.0",
  "description": "New Tab",
  "applications": {
    "gecko": {
      "id": "nathandaven@gmail.com",
      "strict_min_version": "54.0"
    }
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "chrome_settings_overrides": {
    "homepage": "index.html"
  },

  "icons": {
    "512": "logo512.png"
  },
  "permissions": ["*://google.com/*", "*://https://duckduckgo.com/*"],
  "omnibox": {
    "keyword": "newtab"
  },
  "web_accessible_resources": [
    "google.com",
    "duckduckgo.com",
    "https://nathandaven.github.io/startpage/image.gif"
  ],
  "content_security_policy": "script-src 'self' https://duckduckgo.com; object-src 'self' https://duckduckgo.com;",
  "permissions": ["http://*/*", "storage", "activeTab"]
}
